{
    "name": "opentrackmania",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "update:seasons": "ts-node --transpile-only cron/updateSeasons.ts",
        "update:rankings": "ts-node --transpile-only cron/updateRankings.ts",
        "update:totd": "ts-node --transpile-only cron/updateTOTD.ts",
        "start": "ts-node --transpile-only index.ts",
        "watch": "tsnd --respawn index.ts",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "migrate": "sequelize db:migrate",
        "migrate:undo": "sequelize db:migrate:undo",
        "seed:all": "sequelize db:seed:all",
        "seed:undo:all": "sequelize db:seed:undo:all"
    },
    "cacheDirectories": [
        "node_modules"
    ],
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/cors": "^2.8.7",
        "@types/express": "^4.17.7",
        "@types/node": "^14.0.27",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "dotenv-cli": "^3.2.0",
        "express": "^4.17.1",
        "node-cache": "^5.1.2",
        "pg": "^8.3.0",
        "pg-hstore": "^2.3.3",
        "sequelize": "^6.3.3",
        "sequelize-cli": "^6.2.0",
        "trackmania-api-node": "0.0.22",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.0.5",
        "ts-node-dev": "^1.0.0-pre.56"
    }
}
